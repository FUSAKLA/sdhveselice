includes:
	- database.php
	- user.php

parameters:


php:
    date.timezone: Europe/Prague


application:
	errorPresenter: Error
	mapping:
		*: App\*Module\Presenters\*Presenter

session:
	expiration: 14 days



services:
	- App\UserManager(%user.username%, %user.password%)
	- App\RouterFactory
	- App\Article
	- App\GalleryManager
	- App\Model\FileManager
	router: @App\RouterFactory::createRouter
